{
  "name": "react-start-kit",
  "version": "0.0.1",
  "description": "Easy to start a React project~",
  "main": "index.js",
  "scripts": {
    "start": "babel-node --presets=latest --plugins=transform-class-properties,transform-object-rest-spread -- server.js",
    "webpack-build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run webpack-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foisonocean/react-start-kit.git"
  },
  "keywords": [
    "react",
    "start",
    "kit"
  ],
  "author": "foisonocean",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foisonocean/react-start-kit/issues"
  },
  "homepage": "https://github.com/foisonocean/react-start-kit#readme",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "classnames": "^2.2.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
