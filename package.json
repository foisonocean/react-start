    "eslint-plugin-standard": "^2.0.1",
    "inquirer": "^3.0.4",
    "koa": "^2.0.1",
    "koa-mount": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "koa-webpack-middleware": "^1.0.3",
    "npm-run-all": "^4.0.2",
    "ora": "^1.1.0",
